{
  "compilerOptions": {
    "lib": ["deno.ns", "dom", "deno.unstable"]
  },
  "imports": {
    "std/": "https://deno.land/std@0.177.0/",
    "@supabase/supabase-js": "https://esm.sh/@supabase/supabase-js@2.43.4",
    "openai": "https://deno.land/x/openai@v4.24.1/mod.ts",
    "csv": "https://deno.land/std@0.177.0/csv/mod.ts"
  },
  "lint": {
    "files": {
      "include": ["./"],
      "exclude": ["./_shared/"]
    },
    "rules": {
      "tags": ["recommended"],
      "include": [],
      "exclude": []
    }
  },
  "fmt": {
    "files": {
      "include": ["./"],
      "exclude": []
    },
    "options": {
      "useTabs": false,
      "lineWidth": 80,
      "indentWidth": 2,
      "singleQuote": false,
      "proseWrap": "preserve"
    }
  }
}
